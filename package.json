{
  "name": "koa2-redux-universal",
  "version": "1.0.0",
  "description": "koa2 redux universal",
  "main": "./dist/server",
  "scripts": {
    "dev": "better-npm-run dev",
    "compile": "better-npm-run compile",
    "build:client": "npm run clear:client && better-npm-run build:client",
    "build:prod": "npm run clear:prod && npm run compile && npm run build:client",
    "clear:prod": "rimraf dist",
    "clear:client": "rimraf dist/server/static",
    "serve": "pm2-docker start pm2.json --env production",
    "test": "better-npm-run mocha",
    "test:watch": "better-npm-run mocha -- -w",
    "lint": "./node_modules/.bin/eslint .",
    "validate": "npm run lint && npm run test"
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "HOST": "localhost",
        "PORT": "8000",
        "ENTRY": "src"
      }
    },
    "build:client": {
      "command": "webpack --profile --colors --progress --config ./webpack/prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "compile": {
      "command": "babel src -d dist --ignore '/client/' --copy-files && cp -r src/client dist",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "mocha": {
      "command": "./node_modules/.bin/mocha --opts ./test/.mocharc -r ./test/*-spec.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "engines": {
    "node": ">=6.2.2",
    "npm": ">=3.9.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-react-css-modules": "^3.3.2",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "better-npm-run": "^0.0.14",
    "co": "^4.6.0",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.6",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "passport": "^0.3.2",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "better-npm-run": "^0.0.14",
    "chokidar": "^1.7.0",
    "chunk-manifest-webpack2-plugin": "^1.0.1",
    "css-modules-require-hook": "^4.2.2",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "history": "^4.7.2",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-inline-chunk-plugin": "^1.1.1",
    "html-webpack-plugin": "^2.29.0",
    "inline-chunk-manifest-html-webpack-plugin": "^0.1.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-csrf": "^2.x",
