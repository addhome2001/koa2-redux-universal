{
  "name": "koa2-redux-universal",
  "version": "1.0.0",
  "description": "koa2 redux universal for pure server side rendering",
  "scripts": {
    "dev": "better-npm-run dev",
    "compile": "better-npm-run compile",
    "build:client": "npm run clear:client && better-npm-run build:client",
    "build:prod": "npm run clear:prod && npm run compile && npm run build:client",
    "clear:prod": "rimraf build",
    "clear:client": "rimraf build/server/static",
    "serve": "better-npm-run serve",
    "now-build": "yarn build:prod",
    "now-start": "yarn serve",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node bin/server --plugins=dynamic-import-node",
      "env": {
        "NODE_ENV": "development",
        "HOST": "localhost",
        "PORT": "8000",
        "ENTRY": "src",
        "ENABLE_SSR": 0
      }
    },
    "serve": {
      "command": "node bin/server",
      "env": {
        "NODE_ENV": "production",
        "HOST": "localhost",
        "PORT": "8000",
        "ENTRY": "build",
        "ENABLE_SSR": 1
      }
    },
    "build:client": {
      "command": "webpack --profile --colors --progress --config ./webpack/prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "compile": {
      "command": "babel src -d build --copy-files --plugins=dynamic-import-node",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-css-modules": "^5.0.0",
    "ejs": "^2.6.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "v5.0.0-beta.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "autoprefixer": "^9.4.2",
    "babel-loader": "^8.0.4",
    "better-npm-run": "^0.1.1",
    "chokidar": "^1.7.0",
    "connected-react-router": "^5.0.1",
    "css-loader": "^1.0.1",
    "css-modules-require-hook": "^4.2.3",
    "history": "^4.7.2",
