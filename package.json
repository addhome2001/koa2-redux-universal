{
  "name": "koa2-redux-universal",
  "version": "1.0.0",
  "description": "koa2 redux universal",
  "main": "./dist/server",
  "scripts": {
    "dev": "better-npm-run dev",
    "debug": "better-npm-run debug",
    "compile": "better-npm-run compile",
    "build:client": "better-npm-run build:client",
    "build:prod": "npm run clear && npm run compile && npm run build:client",
    "clear": "rimraf dist",
    "serve": "better-npm-run serve",
    "test": "better-npm-run mocha",
    "test:watch": "better-npm-run mocha -- -w",
    "lint": "./node_modules/.bin/eslint .",
    "validate": "npm run lint && npm run test"
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "PORT": "8000",
        "ENTRY": "src"
      }
    },
    "debug": {
      "command": "npm run dev",
      "env": {
        "DEBUG": "*"
      }
    },
    "build:client": {
      "command": "webpack --profile --colors --progress --config ./webpack/prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "compile": {
      "command": "babel src -d dist --ignore client --copy-files",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "serve": {
      "command": "node bin/server",
      "env": {
        "NODE_ENV": "production",
        "PORT": "8000",
        "ENTRY": "dist"
      }
    },
    "mocha": {
      "command": "./node_modules/.bin/mocha --opts ./test/.mocharc -r ./test/*-spec.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "engines": {
    "node": ">=6.2.2",
    "npm": ">=3.9.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-react-css-modules": "^2.6.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "co": "^4.6.0",
    "ejs": "^2.4.2",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "mkdirp": "^0.5.1",
    "passport": "0.3.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.22.1",
    "bcryptjs": "^2.4.3",
    "css-loader": "^0.27.3",
    "css-modules-require-hook": "^4.0.6",
    "es6-promise": "^4.0.5",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.28.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.2.0",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^1.0.3",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^2.x",
    "koa-etag": "^2.1.1",
    "koa-generic-session": "^1.11.5",
    "koa-logger": "^1.3.0",
    "koa-onerror": "^3.0.2",
    "koa-passport": "^3.0.0-rc.3",
    "koa-redis": "^3.0.0",
    "koa-router": "^7.0.1",
    "koa-static-server": "^1.0.0",
    "koa-views": "^5.0.1",
    "koa-webpack": "^0.3.1",
    "lru-cache": "^4.1.0",
    "mocha": "^3.2.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.4",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-logger": "^2.8.2",
    "redux-thunk": "2.2.0",
    "style-loader": "^0.15.0",
    "supertest": "^2.0.0",
    "webpack": "^2.3.0",
    "webpack-notifier": "^1.5.0"
  }
}
